{
    "name": "stasa87/auto-mapper-plus",
    "description": "An AutoMapper for PHP",
    "license": "MIT",
    "type": "library",
    "keywords": ["automapper", "converter", "object-to-object"],
    "authors": [
        {
            "name": "Mark Gerarts",
            "email": "mark.gerarts@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/Immo-Facile/functional-php"
        }
    ],
    "autoload": {
        "psr-4": {
            "AutoMapperPlus\\":  ["src/"]
        }
    },
    "require": {
        "lstrojny/functional-php": "^1.4",
        "php": "^7.4 || ^8.1"
    },
    "require-dev": {
        "symfony/var-dumper": "^3.3",
        "symfony/debug": "^3.3",
        "phpunit/phpunit": "^9.6",
        "elendev/composer-push": "^1.0"
    },
    "config": {
        "allow-plugins": {
            "elendev/composer-push": true
        }
    }
}
